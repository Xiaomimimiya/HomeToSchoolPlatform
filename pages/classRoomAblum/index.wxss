page {
    background-color: #fff;
}

.time {
    font-size: 45rpx;
    margin-left: 10rpx;
    font-weight: 650;
}

.image_box {
    margin-top: 10rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: flex-start;
    width: 100%;
}

.image_item {
    width: 33%;
    padding: 10rpx;
}

.image_item image {
    width: 100%;
    height: 220rpx;
    border-radius: 20rpx;
}